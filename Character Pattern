// A B C D E 
// A B C D E 


#include<iostream>
using namespace std;
int main(){
    int n; 
    cout<<"Enter N :";
    cin>>n;
    for(int row=1;row<=n;row++){
        for(int col=1;col<=n;col++){
            cout<<char(col+64)<<" ";
        }
        cout<<"\n";
    }
    return 0;
}

